<%= form_tag session_create_path, :class => 'sign-in-form' do %>
    <h2>Sign In</h2>
    <%= text_field_tag :email,'', :placeholder => 'Email Address' %>
    <%= password_field_tag :password, '', :placeholder => 'Password' %>
    <%= submit_tag 'Sign In', :class => 'btn btn-primary btn-block' %>
<% end %>
